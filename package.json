{
  "private": true,
  "workspaces": [
    "frui",
    "web"
  ],
  "scripts": {
    "frui": "yarn --cwd frui",
    "web": "yarn --cwd web",
    "build": "yarn build:frui && yarn build:web",
    "build:frui": "yarn --cwd frui build",
    "build:web": "yarn --cwd web build && cp .nojekyll docs/.nojekyll && cp CNAME docs/CNAME"
  }
}