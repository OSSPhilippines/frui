{
  "type": "module",
  "name": "frui",
  "version": "0.1.8",
  "license": "MIT",
  "description": "A collection of vanilla react components written in typescript.",
  "author": "Chris <chris@incept.asia>",
  "homepage": "https://github.com/OSSPhilippines/frui",
  "bugs": "https://github.com/OSSPhilippines/frui/issues",
  "repository": "OSSPhilippines/frui",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./esm/index.js"
    },
    "./Accordion": {
      "require": "./cjs/base/Accordion.js",
      "import": "./esm/base/Accordion.js"
    },
    "./Alert": {
      "require": "./cjs/base/Alert.js",
      "import": "./esm/base/Alert.js"
    },
    "./Badge": {
      "require": "./cjs/base/Badge.js",
      "import": "./esm/base/Badge.js"
    },
    "./Box": {
      "require": "./cjs/base/Box.js",
      "import": "./esm/base/Box.js"
    },
    "./Bread": {
      "require": "./cjs/base/Bread.js",
      "import": "./esm/base/Bread.js"
    },
    "./Button": {
      "require": "./cjs/base/Button.js",
      "import": "./esm/base/Button.js"
    },
    "./Card": {
      "require": "./cjs/base/Card.js",
      "import": "./esm/base/Card.js"
    },
    "./Dialog": {
      "require": "./cjs/base/Dialog.js",
      "import": "./esm/base/Dialog.js"
    },
    "./Dropdown": {
      "require": "./cjs/base/Dropdown.js",
      "import": "./esm/base/Dropdown.js"
    },
    "./Loader": {
      "require": "./cjs/base/Loader.js",
      "import": "./esm/base/Loader.js"
    },
    "./Notifier": {
      "require": "./cjs/base/Notifier.js",
      "import": "./esm/base/Notifier.js"
    },
    "./Pager": {
      "require": "./cjs/base/Pager.js",
      "import": "./esm/base/Pager.js"
    },
    "./Progress": {
      "require": "./cjs/base/Progress.js",
      "import": "./esm/base/Progress.js"
    },
    "./Scope": {
      "require": "./cjs/tool/Scope.js",
      "import": "./esm/tool/Scope.js"
    },
    "./Table": {
      "require": "./cjs/base/Table.js",
      "import": "./esm/base/Table.js"
    },
    "./Tabs": {
      "require": "./cjs/base/Tabs.js",
      "import": "./esm/base/Tabs.js"
    },
    "./Tooltip": {
      "require": "./cjs/base/Tooltip.js",
      "import": "./esm/base/Tooltip.js"
    },
    "./When": {
      "require": "./cjs/tool/When.js",
      "import": "./esm/tool/When.js"
    },
    "./types": {
      "require": "./cjs/types.js",
      "import": "./esm/types.js"
    },
    "./unocss": {
      "require": "./cjs/unocss.js",
      "import": "./esm/unocss.js"
    },
    "./base": {
      "require": "./cjs/base/index.js",
      "import": "./esm/base/index.js"
    },
    "./base/*": {
      "require": "./cjs/base/*.js",
      "import": "./esm/base/*.js"
    },
    "./data/*": {
      "require": "./cjs/data/*.js",
      "import": "./esm/data/*.js"
    },
    "./component": {
      "require": "./cjs/base/index.js",
      "import": "./esm/base/index.js"
    },
    "./component/*": {
      "require": "./cjs/base/*.js",
      "import": "./esm/base/*.js"
    },
    "./form": {
      "require": "./cjs/form/index.js",
      "import": "./esm/form/index.js"
    },
    "./form/*": {
      "require": "./cjs/form/*.js",
      "import": "./esm/form/*.js"
    },
    "./tool": {
      "require": "./cjs/tool/index.js",
      "import": "./esm/tool/index.js"
    },
    "./tool/*": {
      "require": "./cjs/tool/*.js",
      "import": "./esm/tool/*.js"
    },
    "./view": {
      "require": "./cjs/view/index.js",
      "import": "./esm/view/index.js"
    },
    "./view/*": {
      "require": "./cjs/view/*.js",
      "import": "./esm/view/*.js"
    },
    "./frui.css": "./frui.css"
  },
  "typesVersions": {
    "*": {
      ".": [ "./cjs/index.d.ts" ],
      "types": [ "./cjs/types.d.ts" ],
      "unocss": [ "./cjs/unocss.d.ts" ],
      "Accordion": [ "./cjs/base/Accordion.d.ts" ],
      "Alert": [ "./cjs/base/Alert.d.ts" ],
      "Badge": [ "./cjs/base/Badge.d.ts" ],
      "Box": [ "./cjs/base/Box.d.ts" ],
      "Bread": [ "./cjs/base/Bread.d.ts" ],
      "Button": [ "./cjs/base/Button.d.ts" ],
      "Card": [ "./cjs/base/Card.d.ts" ],
      "Dialog": [ "./cjs/base/Dialog.d.ts" ],
      "Dropdown": [ "./cjs/base/Dropdown.d.ts" ],
      "Loader": [ "./cjs/base/Loader.d.ts" ],
      "Notifier": [ "./cjs/base/Notifier.d.ts" ],
      "Pager": [ "./cjs/base/Pager.d.ts" ],
      "Progress": [ "./cjs/base/Progress.d.ts" ],
      "Scope": [ "./cjs/tool/Scope.d.ts" ],
      "Table": [ "./cjs/base/Table.d.ts" ],
      "Tabs": [ "./cjs/base/Tabs.d.ts" ],
      "Tooltip": [ "./cjs/base/Tooltip.d.ts" ],
      "When": [ "./cjs/tool/When.d.ts" ],
      "base": [ "./cjs/base/index.d.ts" ],
      "base/*": [ "./cjs/base/*.d.ts" ],
      "component": [ "./cjs/base/index.d.ts" ],
      "component/*": [ "./cjs/base/*.d.ts" ],
      "data/*": [ "./cjs/data/*.d.ts" ],
      "form": [ "./cjs/form/index.d.ts" ],
      "form/*": [ "./cjs/form/*.d.ts" ],
      "tool": [ "./cjs/tool/index.d.ts" ],
      "tool/*": [ "./cjs/tool/*.d.ts" ],
      "view": [ "./cjs/view/index.d.ts" ],
      "view/*": [ "./cjs/view/*.d.ts" ]
    }
  },
  "files": [
    "cjs",
    "esm",
    "styles",
    "frui.css",
    "LICENSE",
    "README.md"
  ],
  "plugins": [
    "./web/app/plugin",
    "./web/docs/plugin"
  ],
  "scripts": {
    "build": "yarn build:tsc && yarn build:pkg && yarn build:docs",
    "build:pkg": "yarn build:pkg:cjs && yarn build:pkg:esm",
    "build:pkg:cjs": "echo '{\"type\": \"commonjs\"}' > cjs/package.json",
    "build:pkg:esm": "echo '{\"type\": \"module\"}' > esm/package.json",
    "build:tsc": "yarn build:tsc:cjs && yarn build:tsc:esm",
    "build:tsc:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:tsc:esm": "tsc -p ./tsconfig.esm.json",
    "build:docs": "tsx scripts/build.ts",
    "dev": "tsx scripts/develop.ts",
    "test": "vitest --config vitest.config.ts",
    "report": "vitest run --coverage"
  },
  "devDependencies": {
    "@codemirror/language-data": "6.5.1",
    "@stackpress/ingest": "0.6.2",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.0",
    "@types/inputmask": "5.0.7",
    "@types/node": "24.3.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "5.0.2",
    "codemirror": "6.0.2",
    "inputmask": "5.0.9",
    "jsdom": "27.1.0",
    "libphonenumber-js": "1.12.23",
    "markdown-to-jsx": "7.7.13",
    "moment": "2.30.1",
    "r22n": "1.0.10",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-shiki": "0.8.0",
    "react-toastify": "11.0.5",
    "reactus": "0.6.2",
    "tsx": "4.20.5",
    "typescript": "5.9.2",
    "universal-cookie": "8.0.1",
    "unocss": "66.5.1",
    "vite": "7.1.5",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.8"
  },
  "peerDependencies": {
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "dependencies": {
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/user-event": "14.6.1"
  }
}
